#. TRANSLATORS: this needs to be a single rune that is understood to mean "hours" in e.g. 1h30m
#: strutil/quantity/quantity.go:107
msgid "h"
msgstr "o"

#. TRANSLATORS: %d will be at least 2; the singular is only included to help gettext
#: timeutil/human.go:75
#, c-format
msgid "in %d day, at 15:04 MST"
msgid_plural "in %d days, at 15:04 MST"
msgstr[0] "rupan %d antv, 15:04 MST mew"
msgstr[1] "%dwe, 15:04 MST mew"

#. TRANSLATORS: this needs to be a single rune that is understood to mean "minutes" in e.g. 1m30s
#: strutil/quantity/quantity.go:105
msgid "m"
msgstr "m"

#. TRANSLATORS: this needs to be a single rune that is understood to mean "seconds" in e.g. 1m30s
#. (I fully expect this to always be "s", given it's a SI unit)
#: strutil/quantity/quantity.go:103
msgid "s"
msgstr "s"

#: timeutil/human.go:70
msgid "today at 15:04 MST"
msgstr "faci antv 15:04 MST mew"

#: timeutil/human.go:72
msgid "tomorrow at 15:04 MST"
msgstr "wvle 15:04 MST mew"

#. TRANSLATORS: this needs to be a single rune that is understood to mean "years" in e.g. 1y45d
#: strutil/quantity/quantity.go:111
msgid "y"
msgstr "m"

#: timeutil/human.go:68
msgid "yesterday at 15:04 MST"
msgstr "wiya 15:04 MST mew"
