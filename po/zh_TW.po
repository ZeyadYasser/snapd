#. TRANSLATORS: the first %q will be the (quoted) snap name, the second a channel
#: cmd/snap/cmd_snap_op.go:416
#, c-format
msgid "%q switched to the %q channel\n"
msgstr "%q 切換到 %q 頻道\n"

#. TRANSLATORS: 1. snap name, 2. snap version (keep those together please). the 3rd %s is a path (where it's mounted from).
#: cmd/snap/cmd_snap_op.go:1080
#, c-format
msgid "%s %s mounted from %s\n"
msgstr "%s %s 已從 %s 掛載\n"

#. TRANSLATORS: %s is an error message (e.g. “cannot yadda yadda: permission denied”)
#: cmd/snap/error.go:209
#, c-format
msgid "%s (try with sudo)"
msgstr "%s（請以 sudo 重試）"

#: cmd/snap/cmd_snap_op.go:625
#, c-format
msgid "%s already installed\n"
msgstr "%s 已安裝\n"

#: cmd/snap/cmd_snap_op.go:1134
#, c-format
msgid "%s disabled\n"
msgstr "%s 已停用\n"

#: cmd/snap/cmd_snap_op.go:1107
#, c-format
msgid "%s enabled\n"
msgstr "%s 已啟用\n"

#: cmd/snap/cmd_snap_op.go:204
#, c-format
msgid "%s not installed\n"
msgstr "%s 未安裝\n"

#: cmd/snap/cmd_snap_op.go:158 cmd/snap/cmd_snap_op.go:197
#, c-format
msgid "%s removed\n"
msgstr "%s 已被移除\n"

#. TRANSLATORS: first %s is a snap name, second %s is a revision
#: cmd/snap/cmd_snap_op.go:400
#, c-format
msgid "%s reverted to %s\n"
msgstr "%s 已恢復到 %s\n"

#. TRANSLATORS: the args are a snap name optionally followed by a channel, then a version (e.g. "some-snap (beta) 1.3 installed")
#: cmd/snap/cmd_snap_op.go:388
#, c-format
msgid "%s%s %s installed\n"
msgstr "%s%s %s 已安裝\n"

#: cmd/snap/cmd_snap_op.go:850
msgid "--list does not accept additional arguments"
msgstr "--list 無法增加更多的引數"

#: cmd/snap/cmd_run.go:233
msgid "-r can only be used with --hook"
msgstr "-r 只能跟  --hook 使用"

#: cmd/snap/cmd_help.go:208
msgid "...more"
msgstr "...更多"

